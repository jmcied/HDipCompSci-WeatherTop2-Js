<table class="ui fixed table">
  <thead>
  <tr>
    <th>Code</th>
    <th>Temperature</th>
    <th>WindSpeed</th>
    <th>Pressure</th>
  
  </tr>
  </thead>
  <tbody>
     {{#each station.readings}}
      <tr>
        <td>
          {{code}}
        </td>
        <td>
          {{temperature}}
        </td>
        <td>
          {{windSpeed}}
        </td>
        <td>
          {{pressure}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>